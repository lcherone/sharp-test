{
    "version": 2,
    "name": "sharp-test",
    "builds": [
      {"src": "src/index.js", "use": "@now/node-server"}
    ],
    "routes": [
      {"src": "^/api/images", "dest": "src/index.js"},
      {"src": "^/images/(.*)", "dest": "src/index.js"},
      {"src": "^/upload", "dest": "src/index.js"},
      {"src": "^/css/style.css", "dest": "src/index.js"},
      {"src": "^/js/app.js", "dest": "src/index.js"},
      {"src": ".*", "dest": "src/index.js"}
    ]
  }
  